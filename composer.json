{
  "name": "afbora/kirby-template-hooks",
  "description": "Enable page and file template hooks for Kirby 3",
  "keywords": [
    "kirby",
    "kirby-3",
    "kirby3-plugin",
    "kirby-plugin"
  ],
  "version": "1.2.1",
  "type": "kirby-plugin",
  "license": "MIT",
  "homepage": "https://github.com/afbora/kirby-template-hooks",
  "support": {
    "email": "ahmet@getkirby.com"
  },
  "authors": [
    {
      "name": "Ahmet Bora",
      "email": "ahmet@getkirby.com",
      "homepage": "https://owebstudio.com"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": "^7.4|^8.0",
    "getkirby/composer-installer": "^1.2"
  },
  "extra": {
    "installer-name": "kirby-template-hooks"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
